# WenXin MoYun Frontend - Production Environment Variables
# These will be set during Cloud Build deployment

# API Configuration
VITE_API_BASE_URL=https://wenxin-moyun-api-asia-east1.run.app
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# CDN Configuration
VITE_CDN_URL=https://storage.googleapis.com/wenxin-moyun-prod-static

# Environment
VITE_ENVIRONMENT=production
VITE_DEBUG=false

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Google Services (Optional)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX